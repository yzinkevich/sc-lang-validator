{
  "name": "localization-validator",
  "version": "1.0.0",
  "description": "Приложение для валидации локализационных ключей",
  "main": "dist/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./dist/main.js",
    "start:debug": "cross-env NODE_ENV=production electron ./dist/main.js --debug",
    "build": "webpack --config webpack.main.config.js --progress && webpack --config webpack.preload.config.js --progress && webpack --config webpack.renderer.config.js --progress --stats-error-details",
    "clean": "rm -rf dist node_modules .cache package-lock.json",
    "rebuild": "npm run clean && npm install && npm run build",
    "build:main": "webpack --config webpack.main.config.js",
    "build:preload": "webpack --config webpack.preload.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js --progress",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack serve --config webpack.renderer.config.js\" \"wait-on http://localhost:8080 && cross-env NODE_ENV=development electron .\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@mui/styles": "^5.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@emotion/types": "^0.7.0",
    "@types/electron": "^1.6.12",
    "@types/node": "^20.17.9",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.8.8",
    "electron-builder": "^23.6.0",
    "html-webpack-plugin": "^5.5.1",
    "typescript": "^5.7.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
